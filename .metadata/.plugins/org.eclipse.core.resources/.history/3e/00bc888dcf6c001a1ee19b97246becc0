package array03;

import java.util.Scanner;

public class Test1 {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		
		for (int i = 1; i <= num; i = i + 2) {
			int a = num/2;
			for (int j = 0; j < a; j++) {
				System.out.print(a);
			}
			System.out.println();
		}

	}
}
