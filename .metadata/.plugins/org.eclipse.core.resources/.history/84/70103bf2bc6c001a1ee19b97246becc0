package array03;

import java.util.Scanner;

public class Gugudan2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("임의의 숫자를 입력하세요.");
		int num = sc.nextInt();

		for (int i = 0; i < 10; i++) {
			for (int j = num; j<num; j++) {
				System.out.println("*");
			}
			System.out.println();
		}
	}
}
