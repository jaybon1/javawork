package ch05;

class Car {

	void run() {
		System.out.println("기본 움직임");
	}

}

class Sonata extends Car {
	
	@Override  // 어노테이션 // 오버라이드 : 부모 클래스의 메서드를 무효화 시키는 것
	void run() {
		super.run(); // super는 부모의 heap
	}
}

public class CarEx01 {

	public static void main(String[] args) {
		Sonata s1 = new Sonata();
		s1.run();
	}

}
