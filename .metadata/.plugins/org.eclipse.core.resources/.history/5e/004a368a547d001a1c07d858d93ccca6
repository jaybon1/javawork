package ch13;

class A {
	int a = 10;
}

public class Test {
	
	static void sample(A a) {
		System.out.println(a.a);
	}
	
	
	public static void main(String[] args) {
	A test = new A();
	A test1 = new A();
	
	test.a = test.a + 10;
	
	
	System.out.println(test.a);
	
	System.out.println(test1.a);
	
	}
}
