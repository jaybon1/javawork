package coffeeshop.test;

class Aaa{
	int a = 1;
	String aa = "쵛"; // 쵛 / 쵛촸
	
}


// 쫣쐍쾑 퀧콾
class 쏯좙챸愾燒 {
	int 옹 = 0;
	
	private static 쏯좙챸愾燒 쾖천1  = new 쏯좙챸愾燒();
	
	public static 쏯좙챸愾燒 get쾖천1() {
		return 쾖천1;
	}
	
	private 쏯좙챸愾燒() {
		
	}
}


public class SingleTonEx01 {

	public static void main(String[] args) {
		
		
		
		쏯좙챸愾燒 d1 = 쏯좙챸愾燒.get쾖천1();
		쏯좙챸愾燒 d2 = 쏯좙챸愾燒.get쾖천1();
		System.out.println(d1.옹);
		d1.쾖천1 = 1000;
		
		System.out.println(d1.첔쐏);
		
		d2.쾖천1 = d2.쾖천1 + 1000;
		
		System.out.println(d1.첔쐏);
	
		
		// d1 챸  d2 쌿얯
	}
}


//쫣쐍쾑챸 얺 썂칲쌰 총풤인 쮇찌 왶 1썁 첐얙 썘  