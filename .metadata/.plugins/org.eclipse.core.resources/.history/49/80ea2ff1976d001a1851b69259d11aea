package array03;

public class StarEx02 {
	
	static void printStars(int i) {
		
	}
	
	public static void main(String[] args) {
		System.out.println("___"+"*"); // 공백 3 별 1
		System.out.println("__"+"**"); // 공백 2 별 2
		System.out.println("_"+"***"); // 공백 1 별 3
		System.out.println(""+"****"); // 공백 0 별 4
	}
}
