package composite;

public class 라면1 {
	String item1;
	String item2;
	String item3;
	
	public 라면1() {
		this.item1 = "물";
		this.item2 = "건더기스프";
		this.item3 = "분말스프";
	}

	
	public 라면1(String item1, String item3) {
		this(item1, null, item3);
	}
	
	public 라면1(String item1, String item2, String item3) {
		this.item1 = item1;
		this.item2 = item2;
		this.item3 = item3;
	}
	
	public static void main(String[] args) {
		라면1 l1 = new 라면1("물", "건더기스프", "분말스프");
		라면1 l2 = new 라면1();
		라면1 l3 = new 라면1("쌀뜨물", "건더기스프", "분말스프");
	}
}
