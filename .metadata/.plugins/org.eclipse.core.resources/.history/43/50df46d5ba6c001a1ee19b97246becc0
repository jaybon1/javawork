package array03;

import java.util.Scanner;

public class Gugudan2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("2~9를 입력하세요.");
		int num = sc.nextInt();

		for (int i = 1; i < 9; i++) {
			System.out.println(num + "*" + i + "=" + (num * i));
		}
	}
}
