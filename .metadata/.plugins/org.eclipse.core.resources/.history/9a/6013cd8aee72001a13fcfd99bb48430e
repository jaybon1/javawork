package ch05;

// 어댑터
abstract class 칼 {
	abstract void kill();

	abstract void cook();

	abstract void repair();
}

class 백종원 extends 칼 {

	@Override
	void kill() {
	}

	@Override
	void cook() {
		System.out.println("요리하기");
	}

	@Override
	void repair() {
	}

}

class 요리사 {

}

