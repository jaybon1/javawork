package ch03;

// 자바 기준
// 메소드(함수)는 클래스 안에 작성할 수 있다.
// 메소드(함수)는 메소드 (함수)안에 작성할 수 없다.

public class MethodEx01 {

	// 전역공간

	// void는 리턴하지 않는다는 뜻이다

	static int add(int n1, int n2) {
		int result = n1 + n2;
		// System.out.println("result : " + result);
		return result; // 특징: return은 가장 마지막에!
	}

	static int minus(int n1, int n2) {
		return n1 - n2;
	}

	static int multi(int n1, int n2) {
		return n1 * n2;
	}

	static int devide(int n1, int n2) {
		return n1 / n2;
	}

	public static void main(String[] args) {

		int num = MethodEx01.add(5, 3);
		System.out.println("더하기 : " + num);

		System.out.println("빼기    : " + minus(5, 3));

		System.out.println("곱하기 : " + multi(5, 3));

		System.out.println("나누기 : " + devide(5, 3));

	}
}
