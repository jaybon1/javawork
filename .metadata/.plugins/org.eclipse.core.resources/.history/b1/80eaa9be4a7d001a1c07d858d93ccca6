package ch13;

class MoneyChange{
	int money = 10000;
	
	public void accept() {
		// 은행에 인출요청을해서 2만원을 받을 예정 2초
		new Thread(new Runnable() {
			
			@Override
			public void run() {
				try {
					Thread.sleep(2000);
				} catch (InterruptedException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}
			}
		}).start();
	}
}

