package ch05;

class 사람 {
	int num = 10;
}

class 아시아인 extends 사람 {
	
}

class 한국인 extends 아시아인 {

}

class 박서준 extends 한국인 {

}

public class EveryPerson {
	public static void main(String[] args) {
		
		박서준 b1 = new 박서준();
		System.out.println(b1.num);
		
		한국인 b2 = new 박서준();
		
		사람 b3 = new 한국인();
	}
}
