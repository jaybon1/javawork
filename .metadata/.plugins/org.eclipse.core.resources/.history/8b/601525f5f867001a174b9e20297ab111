package ch02;

public class VarEx03 {

	public static void main(String[] args) {
		char s1 = '가'; // 2바이트
		System.out.println(s1);

		s1 = '나';
		System.out.println(s1);

		char c1 = '가';
		char c2 = '나';
		char c3 = '다';
		System.out.print(c1);
		System.out.print(c2);
		System.out.print(c3);
		// 자료형 뒤에 []를 넣어 배열을 만듬
		char[] str = { '가', '나', '다' };
	}
}
