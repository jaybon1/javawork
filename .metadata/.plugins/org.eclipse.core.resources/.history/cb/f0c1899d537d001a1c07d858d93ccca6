package ch13;

class A {
	int a = 10;
}

public class Test {
	
	static void sample(A a) {
		System.out.println(a.a);
	}
	
	
	public static void main(String[] args) {
		
	A test = new A();
	
	
	A test1 = test;
	A test2 = test1;
	
	
	// Èü 

	System.out.println(new A().a);
	System.out.println(test1.a);
	System.out.println(test2.a);
	}
}
