package coffeeshop.test;

class Data1 {
	// 오브젝트는 받기는 쉬우나 받은뒤 사용하려면 캐스팅 해야하기 때문에 불편
	Object data;
}

class Data <T> {
	T value;
}


public class GenericEx01 {
	public static void main(String[] args) {
		Data<String> data = new Data();
		data.value = "문자열";
		System.out.println(data.value);
		
		// 제네릭에는 기본자료형이 들어갈 수 없다
		// Wrapper 클래스 (기본자료형의 수만큼 있음)
		Data<int> data2 = new Data(); 
	}
}
