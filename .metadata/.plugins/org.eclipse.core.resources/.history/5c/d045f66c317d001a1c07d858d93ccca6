package ch13;

class Sub implements Runnable{
	@Override
	public void run() {
		// TODO Auto-generated method stub
		
	}
}

public class ThreadEx01 {

	// 메인 쓰레드
	public static void main(String[] args) {
		
		Thread t1 = new Thread();
		t1.start();
		
		for (int i = 1; i < 6; i++) {
			System.out.println("메인 쓰레드 : " + i);
			try {
				Thread.sleep(1000);
			} catch (Exception e) {
				e.printStackTrace();
			} // 자고있는 이 쓰레드를 깨우는 것을 간섭이라고 한다
		}
	}
}
