package array03;

public class StarEx02 {

	static void printStars(int i) {
		for (int k = 0; k <= i; j++) {
			
		}
			for (int j = 0; j < 4 - i; j++) {
				System.out.print("_");
			}
			for (int j = 0; j < i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

	public static void main(String[] args) {
		printStars(10);

	}
}
