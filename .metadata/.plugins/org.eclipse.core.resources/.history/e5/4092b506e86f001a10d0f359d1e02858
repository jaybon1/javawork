package stars;

class Zealot {
	final String name;
	int hp;
	int attack;

	public Zealot(String name, int hp, int attack) {
		this.name = name;
		this.hp = hp;
		this.attack = attack;
	}

}

class Dragoon {
	final String name;
	int hp;
	int attack;

	public Dragoon(String name, int hp, int attack) {
		this.name = name;
		this.hp = hp;
		this.attack = attack;
	}

}

public class GameStart {
	public static void main(String[] args) {
		Zealot z1 = new Zealot("1¹øÁú·µ", 100, 10);
		
	}
}
