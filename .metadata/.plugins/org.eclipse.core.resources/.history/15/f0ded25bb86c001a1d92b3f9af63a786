package array03;

public class Gugudan {

	static void startGugudan(int n) {
		for (int i = 1; i < 10; i++) {
			System.out.println(n + "*" + i + "=" + (n * i));
		}
		System.out.println();
	}

	public static void main(String[] args) {
		for (int i = 2; i < 10; i++) {
			for (int i = 1; i < 10; i++) {
				System.out.println(n + "*" + i + "=" + (n * i));
			}
			System.out.println();
		}
	}
}
