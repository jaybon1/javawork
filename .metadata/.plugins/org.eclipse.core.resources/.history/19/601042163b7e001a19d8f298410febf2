package ch02;

class 우리집 {
	String 집이름;
	int 평수;
	int 층수;
	String 주소;
	
}

public class Class1 {
	public static void main(String[] args) {
		
		
		우리집 a = new 우리집();
		
		a.setClass();
		
		System.out.println(a.집이름);
		System.out.println(a.평수);
		System.out.println(a.층수);
		System.out.println(a.주소);

		
	}
}