package stars1.zerg;

import stars1.Behavior;
import stars1.protoss.Dragoon;
import stars1.protoss.Zealot;

public abstract class Zerg implements Behavior{
	
	@Override
	public void move() {
		System.out.println("이동");
		
	}
	
	@Override
	public void repair() {
		System.out.println("쉴드 치료");
		
	}
	
	public static void upgrade() {
		Zealot.attack++;
		Dragoon.attack++;
	}
}
