package ch05;

class A {
	void run() {
		System.out.println("A 달린다.");
	}
}

class B extends A {
	@Override
	void run() {
		System.out.println("B 달린다.");
	}
}

class C extends B {
	@Override
	void run() {
		System.out.println("C 달린다.");
	}
}

public class OverrideEx01 {
	public static void main(String[] args) {
		A c1 = new C();
	}
}
