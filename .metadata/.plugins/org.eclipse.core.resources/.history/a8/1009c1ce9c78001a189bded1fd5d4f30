import com.google.gson.Gson;

public class NetworkEx0201 {
	
	public static void main(String[] args) {

		try {
			// 1번 주소 객체 만들기
			URL url = new URL("여기에 주소를넣으세요");
			
			// 2번 스트림 연결
			HttpURLConnection con = (HttpURLConnection) url.openConnection();
		} catch (Exception e) {
			e.printStackTrace();
		}

	}
}
