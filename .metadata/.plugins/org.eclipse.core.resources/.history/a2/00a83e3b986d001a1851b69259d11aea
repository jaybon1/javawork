package array03;

public class StarEx02 {
	
	static void printStars(int i) {
		for (int j = 0; j < 4; j++) {
			if(i == 1) {
				System.out.print("___");
			}
			if(i == 2) {
				System.out.print("_");
			}
			if(i == 3) {
				System.out.print("_");
			}
			if(i == 4) {
				System.out.print("");
			}
		}
	}
	
	public static void main(String[] args) {

	}
}
