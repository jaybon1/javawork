package ch13;

class Car {
	void run() {
		System.out.println("자동차는 달린다");
	}
}

class Sonata extends Car {
	@Override
	void run() {
		super.run();
	}
}

class Granger extends Car {
	@Override
	void run() {
		super.run();
	}
}



public class AnomyEx03 {
	
	static void start(Car car) {
		car.run();
	}
	
	public static void main(String[] args) {
		start(new Car());
	}
}
