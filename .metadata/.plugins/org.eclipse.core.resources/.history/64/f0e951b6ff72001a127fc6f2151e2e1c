package stars1;

// 인터페이스는 추상 클래스와 유사
// 다른점 : 추상 메서드만 존재가능
// 동적 바인딩의 통로로만 사용

public interface Behavior {
	public void move() {
		System.out.println("이동");
	}

	public void repair() {
		System.out.println("쉴드 치료");
	}

	// 공격은 근거리 원거리 등 다르기때문에 추상 메서드로만 생성
	// public abstract void attack();

	public static void upgrade() {
		Zealot.attack++;
		Dragoon.attack++;
	}
}
