package mask;

class Test4{
	private int num1; //선언
	private int password = 1234;
	
	public int getNum1(int password) {
		if(this.password == password) {
			return num1;			
		} else {			
			System.out.println("비밀번호가 틀렸습니다");
		}
		return 0;
	}
	
	public void setNum1(int num1) {
		this.num1 = num1;
	}
	
	
}

public class Test3 {
	private int num; //선언
	
	
	public static void main(String[] args) {
		Test4 t = new Test4();
		
		t.setNum1(10000);
		
		System.out.println(t.getNum1(14444));
		
		
	}
}
