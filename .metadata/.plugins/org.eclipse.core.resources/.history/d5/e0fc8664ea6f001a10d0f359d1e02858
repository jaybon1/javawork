package stars;

class Zealot {
	final String NAME; // 한번 초기화하면 Read Only , 대문자로 적는 것이 약속
	int hp;
	static int attack;

	public Zealot(String name) {
		this.NAME = name;
		this.hp = 100;
		this.attack = 10;
	}

}

class Dragoon {
	final String NAME;
	int hp;
	static int attack;

	public Dragoon(String name) {
		this.NAME = name;
		this.hp = 100;
		this.attack = 10;
	}

}

public class GameStart {
	public static void main(String[] args) {
		//공격력 업그레이드하기
		
		
		//질럿 생성하기
		Zealot z1 = new Zealot("1번질럿", 10);
		
	}
}
