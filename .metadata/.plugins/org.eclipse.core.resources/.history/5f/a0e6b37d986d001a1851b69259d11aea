package array03;

public class StarEx02 {

	static void printStars(int i) {
		if (i == 1) {
			System.out.print("___");
			System.out.print("*");
		}
		if (i == 2) {
			System.out.print("_");
			System.out.print("**");
		}
		if (i == 3) {
			System.out.print("_");
			System.out.print("***");
		}
		if (i == 4) {
			System.out.print("");
			System.out.print("****");
		}
	}

	public static void main(String[] args) {
		printStars(1);
	}
}
