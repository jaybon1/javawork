package ch13;

class A {
	int a = 10;
}

public class Test {
	
	static void sample(A a) {
		System.out.println(a.a);
	}
	
	
	public static void main(String[] args) {
	int a1 = 10;
	int a2 = a;
	a1 = a1 + 10;
	
	A test = new A();
	A test1 = test;
	
	
	System.out.println(test.a);
	
	System.out.println(test1.a);
	
	}
}
