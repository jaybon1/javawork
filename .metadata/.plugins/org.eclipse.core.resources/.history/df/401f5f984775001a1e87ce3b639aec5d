package composite;

// 자바는 다중 상속이 안됨 (부모가 여럿이 될 수 없음)

public class BigBurgerSet {
	// 콤포지트
	private BigBurger bigBurger;
	private Coke coke;
	private FrenchFried frenchFried;
	
	public BigBurgerSet() {
		this.bigBurger = new BigBurger(3500, "세트빅버거");
		this.coke = new Coke(1000, "세트코카콜라");
		this.frenchFried = new FrenchFried(1500, "세트감자튀김");
		System.out.println("세트 가격은 " + (bigBurger.getPrice() + coke.getPrice() + frenchFried.getPrice()) + " 입니다.");
	}
	public static void main(String[] args) {
		BigBurgerSet bSet = new BigBurgerSet();
	}
}
