package ch01;

public class Test {
	public static void main(String[] args) {
		int a = 3;
		int b = 4;
		
		int c = a;
		
		for (int i = 0; i < b - 1; i++) {
			a = a * c;
		}
		
		System.out.println(a);
		
		int[][] val = new int[4][3];
		int aaa = 1;
		for (int i = 0; i < 4; i++) {
			for (int j = 0; j < 3; j++) {
				val[i][j] = a;
				aaa++;
			}
		}
		
		System.out.println(val);
	}
}
